{
    "name": "shuxx/filament-navigation",
    "description": "Configure Filament navigation via a simple PHP config file with groups, links, and 24+ separator styles - no manual navigation building required!",
    "keywords": [
        "filament",
        "laravel",
        "navigation",
        "menu",
        "filament-plugin",
        "sidebar"
    ],
    "homepage": "https://github.com/shuxx/filament-navigation",
    "support": {
        "issues": "https://github.com/shuxx/filament-navigation/issues",
        "source": "https://github.com/shuxx/filament-navigation"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Shuxx",
            "email": "contact@pierre-desforges.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "filament/filament": "^4.0",
        "illuminate/contracts": "^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Shuxx\\FilamentNavigation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shuxx\\FilamentNavigation\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shuxx\\FilamentNavigation\\FilamentNavigationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
